let g:ycm_global_ycm_extra_conf = '/home/maia/.vim/.ycm_extra_conf.py'
let g:ycm_confirm_extra_conf = 0
let g:ycm_autoclose_preview_window_after_completion = 1
let g:ycm_auto_trigger = 1
set shiftwidth=4
set tabstop=4
set autoindent 
set smartindent
syntax on
set number
colo elflord
set encoding=utf-8
